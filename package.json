{
  "name": "zimi-components",
  "version": "1.0.0",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father-build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "release": "npm run build && npm publish",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "changelog": "conventional-changelog -p angular -i ./CHANGELOG.md -s -r 0"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "lint-staged": {
    "*.{js,jsx,md,json}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.less": [
      "prettier --write",
      "stylelint --fix"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "author": "kokiy",
  "homepage": "git@github.com:kokiy/zimi-components.git",
  "license": "MIT",
  "repository": "git@github.com:kokiy/zimi-components.git",
  "keywords": [
    "react-component",
    "zimi-component"
  ],
  "dependencies": {
    "dayjs": "^1.9.7",
    "normalize.css": "^8.0.1",
    "react": "^16.12.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@umijs/fabric": "^2.4.5",
    "@umijs/test": "^3.0.5",
    "conventional-changelog-cli": "^2.1.1",
    "dumi": "1.1.0-rc.8",
    "dumi-theme-mobile": "^1.0.0-rc.4",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "husky": "^4.3.6",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1"
  }
}
